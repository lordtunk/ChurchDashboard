{
  "manifest_version": 2,

  "name": "GCB User Copy",
  "description": "This extension copies user data from a spreadsheet to a new offline user",
  "version": "1.0",
  "background": {
	"scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["*://*/manage-person.html*", "*://guidechurch.onthecity.org/admin/users*", "*://*.planningcenteronline.com/*"],
    "js": ["content.js"],
    "all_frames": true
  }],
  "permissions": [
	"storage",
	"tabs",
	"activeTab",
	"background"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
